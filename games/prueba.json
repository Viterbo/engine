{
    "classes": {
        "scene-ready-tweeners": {
            "on": [
                "scene.ready"
            ],
            "time": 1000
        }
    },
    "scenes": {
        "test": {
            "children": {
                "A": {
                    "type": "BitmapData",
                    "fillStyle": "#FFF",
                    "width": "100%",
                    "height": "12%",
                    "position": {
                        "my": "top left",
                        "at": "top left",
                        "of": "parent"
                    },
                    "behaviors": {
                        "FadeIn": {
                            "class": "scene-ready-tweeners"
                        },
                        "MoveTo": {
                            "class": "scene-ready-tweeners"
                        }
                    }
                }
            }
        }
    }
}